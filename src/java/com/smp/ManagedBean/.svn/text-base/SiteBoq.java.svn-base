package com.smp.ManagedBean;

/**
 *
 * @author smp
 */
public class SiteBoq {

    private ProjectSiteInterface projectSiteInterface;
    private int surplus;
    private int shortage;

    public ProjectSiteInterface getProjectSiteInterface() {
        return projectSiteInterface;
    }

    public void setProjectSiteInterface(ProjectSiteInterface projectSiteInterface) {
        this.projectSiteInterface = projectSiteInterface;
    }

    public int getShortage() {
        return shortage;
    }

    public void setShortage(int shortage) {
        this.shortage = shortage;
    }

    public int getSurplus() {
        return surplus;
    }

    public void setSurplus(int surplus) {
        this.surplus = surplus;
    }
}
